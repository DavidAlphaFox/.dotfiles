#include "$HOME/.Xresources-solarized"
!Xft.dpi: 121
Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintfull
Xft.rgba: rgb

!Xcursor.theme: whitelarge
Xcursor.size: 32
XTerm*loginShell: true
XTerm*locale: UTF-8
XTerm*scrollBar: false
*xterm.termName: xterm-256color
XTerm*faceName: xft:monofur:pixelsize=22
XTerm*faceNameDoublesize: xft:WenQuanYi Bitmap Song:pixelsize=18:antialias=True
XTerm*metaSendsEscape: true
XTerm*highlightSelection: true
XTerm*trimSelection: true
XTerm*locale: true
XTerm*xftAntialiass: true
XTerm*cjkWidth: false
XTerm*bellIsUrgent: true
XTerm*allowBoldFont: true
XTerm*eightBitInput: true
XTerm*backarrowKey: false
XTerm*openIm: true
*inputMethod: ibus
*.vt100.font: fixed
*.vt100.encodingMode: true

#include ".Xresources-solarized"
!Xft.dpi: 121
Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintfull
Xft.rgba: rgb

!Xcursor.theme: whitelarge
Xcursor.size: 32
XTerm*scrollBar: false
XTerm*utf8: 2
*xterm.termName: xterm-256color
XTerm*faceName: xft:Dejavu Sans Mono:pixelsize=18
XTerm*faceNameDoublesize: xft:wenquanyi bitmap song:pixelsize=18:antialias=True
XTerm*metaSendsEscape: true
XTerm*highlightSelection: true
!XTerm*trimSelection: true
XTerm*locale: utf8
XTerm*xftAntialiass: true
XTerm*cjkWidth: false
XTerm*bellIsUrgent: true
XTerm*allowBoldFont: false
*xterm*eightBitInput: false
*XTerm*backarrowKey: false
*XTerm*VT100.Translations: #override \
    <BtnUp>: select-end(CLIPBOARD,PRIMARY,CUT_BUFFER0) \n\
    Ctrl <KeyPress> P: print() \n\
    <Key>BackSpace: string(0x7f) \n\
    <Key>Delete:    string("\033[3~")

XTerm*selectToClipboard:true
*XTerm*ttyModes: erase ^?

AddToMenu cde_edit	"Editors"	Title
+	%mini.edit.xpm%"mined"		Exec exec xfce4-terminal -e mined
+	%mini.xfw.xpm%"Xfw"	        Exec exec xfw
+	%mini.gvim.xpm%"Gvim"		Exec exec gvim
+	%mini.gvim.xpm%"Vim"		Exec exec xfce4-terminal -e vim
+	""		Nop
+ 	%pin.xpm%"Pin Menu" TearMenuOff

AddToMenu cde_file	"File Managers"	Title
+	%mini.xfm_fig.xpm%"nnn"	        Exec exec xfce4-terminal -e nnn
+	%mini.xfe.xpm%"Xfe"		        Exec exec xfe
+	%mini.pcmanfm.xpm%"PcmanFM"	Exec exec pcmanfm
+	""		Nop
+ 	%pin.xpm%"Pin Menu" TearMenuOff

AddToMenu cde_www	"Browsers"	Title
+	%mini.w3m.xpm%"w3m"	 	    Exec exec xterm -e w3m -graph -F -cookie www.google.com
+	%mini.dillo.xpm%"Dillo"	            Exec exec dillo
+	%mini.firefox.xpm%"Firefox"	    Exec exec firefox
+	""		Nop
+	%mini.gopher.xpm%"Gopher" \
          Exec exec xterm -e gopher gopher://gopher.floodgap.com/1/v2
+   ""      Nop
+ 	%pin.xpm%"Pin Menu" TearMenuOff

AddToMenu cde_mail	"News and Mail"	Title
+	%mini.sylpheed.xpm%"Sylpheed" Exec exec sylpheed
+	%mini.mutt.xpm%"Mutt (Outlook)"	\
                         Exec exec urxvtc -e neomutt -F ~/.mutt/muttrc_outlook
+	%mini.mutt.xpm%"Mutt (Fastmail)" \
                         Exec exec urxvtc -e neomutt -F ~/.mutt/muttrc_fastmail
+   ""    Nop                         
+   %mini.xpn.xpm%"XPN"             Exec exec xpn
+   %mini.usenet.xpm%"tin" \
                    Exec exec env NNTPSERVER=news.eternal-september.org xterm -e rtin -Aa
+   ""    Nop
+	%mini.news.xpm%"Newsboat"	             Exec exec xfce4-terminal -e newsboat
+	%mini.liferea.xpm%"Liferea"	             Exec exec liferea
+	""		Nop
+ 	%pin.xpm%"Pin Menu" TearMenuOff

